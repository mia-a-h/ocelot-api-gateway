{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7130"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/auth/register",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/auth/register",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6001
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "PeriodTimespan": 60,
        "Limit": 5
      }
    },
    {
      "UpstreamPathTemplate": "/auth/token",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/auth/token",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6001
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "PeriodTimespan": 60,
        "Limit": 10
      }
    },
    {
      "UpstreamPathTemplate": "/auth/refresh",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/auth/refresh",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 6001
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10m",
        "PeriodTimespan": 600,
        "Limit": 5
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "Auth",
      "Config": [
        {
          "Name": "Auth API",
          "Url": "https://localhost:6001/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
